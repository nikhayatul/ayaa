/* mobile */
@media screen and (max-width: 414px){
   #wrapper{
   	width: 100%;
   	margin: 0 auto;
   }
   #header form{
   	margin-top: -80px;
   	display: none;
   }
   #header{
   	height: 100%;
   	padding: 5px;
   	margin: 20px 0;
   	border-radius: 10px;
   }
   #menu-utama li {
   	display: inline-block;
   	margin-left: -10px;
   }
   #myform {
   	display: grid;
   	grid-template-columns: 1fr;
   }
   #daftar artikel {
   	width: 100%;
   	float: inherit;
   }
   .artikel {
   	border-radius: 10px;
   }
   .artikel b {
   	font-size: 10px;
   }
   .artikel img{
   	width: 90px;
   	height: 115px;
   }
   #sidebar{
   	width: 100%;
   	float: inherit;
   }
   #sidebar h2{
    text-align: center;
   }
   #sidebar p{
   	padding: 5px;
   	text-align: center;
   }
}

/* tablet */
@media screen and (max-width: 568px){
	#wrapper{
		width: 100%;
		margin: 0 auto;
	}
	#header form {
		display: none;
	}
	#header {
		height: 100%;
		padding: 5%;
		margin: 20px 0;
		border-radius: 10px;
	}
	#menu-utama li{
		display: inline-block;
	}
	#daftar-artikel {
         width: 100%;
         float: inherit;
	}
	.artikel {
		border-radius: 10px;
	}
	#sidebar{
		width: 100%;
		float: inherit;
	}
	#sidebar h2{
		text-align: center;
	}
	#sidebar p{
		padding: 5px;
		text-align: center;
	}
}
